{
  "name": "fornt",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf ./public/js/*",
    "back-clean": "rimraf ../src/main/resources/static/* ../src/main/resources/templates/*",
    "build": "npm run clean && webpack -p --progress",
    "back-copy": "npm-run-all build back-clean back-copy:template back-copy:static",
    "back-copy:template": "cpx ./public/*.html ../src/main/resources/templates",
    "back-copy:static": "cpx ./public/**/{*.css,*.js} ../src/main/resources/static"
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "cpx": "^1.5.0",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.6.0",
    "yarn": "^1.1.0"
  }
}
